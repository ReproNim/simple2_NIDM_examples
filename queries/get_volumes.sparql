SELECT DISTINCT *
WHERE                                                                                                                      
{ 
   ?e a onli:assessment-instrument; 
      prov:wasGeneratedBy ?act;
      ncicb:Age ?Age;
      obo:handedness ?handedness;
      full4_iq: ?FIQ;
      ndar:gender ?Gender .

  ?act a onli:instrument-based-assessment;
    dct:isPartOf/dct:isPartOf [ a nidm:Project;
                                dctypes:title ?study];
    prov:wasAssociatedWith [ a prov:Person;
                             ndar:src_subject_id ?ID ] .
  
}


SELECT DISTINCT *
WHERE                                                                                                                      
{ 
    ?fs_stats prov:wasGeneratedBy ?fs_act;
          ?measure ?value .

    ?measure rdfs:label ?label;
        isAbout: ?structure_uri;
        hasMeasurementType: ?measure_uri;
        hasDatumType: ?datum_uri .
    OPTIONAL {?measure hasLaterality: ?laterality }

    ?measure_uri rdfs:label ?measure .
    ?datum_uri rdfs:label ?datum .
          
    ?fs_act a prov:Activity ;
      prov:qualifiedAssociation [prov:wasAssociatedWith [ndar:src_subject_id ?ID]],
                                [prov:wasAssociatedWith [nidm:NIDM_0000164 ?tool]] .
} LIMIT 100

SELECT DISTINCT *
WHERE                                                                                                                      
{ 
    ?fs_stats prov:wasGeneratedBy ?fs_act;
          ?measure ?value .

    ?measure rdfs:label ?label;
        isAbout: ?structure_uri;
        hasMeasurementType: ?measure_uri;
        hasDatumType: ?datum_uri .
    ?measure_uri rdfs:label ?measure .
    ?datum_uri rdfs:label ?datum .
    OPTIONAL {?measure hasLaterality: ?laterality }
          
    ?fs_act a prov:Activity ;
      prov:qualifiedAssociation [prov:wasAssociatedWith [ndar:src_subject_id ?ID]],
                                [prov:wasAssociatedWith [nidm:NIDM_0000164 ?tool]] .
}

SELECT DISTINCT ?ID ?Gender ?diagnosis ?Age ?vol ?software ?region ?project
WHERE
{
  ?bvol_act prov:usedEntity ?t1_entity; 
            prov:qualifiedAssociation [ prov:agent ?agent ] .

  ?agent rdf:type prov:Agent; 
         ndar:src_subject_id ?ID .

  ?activity prov:wasAssociatedWith ?agent;
            dct:isPartOf ?session .

  ?ass_activity dct:isPartOf ?session . 
  ?session dct:isPartOf [ dctypes:title ?project ] . 
  ?ass_entity prov:wasGeneratedBy ?ass_activity;
              rdf:type onli:assessment-instrument; 
              ncicb:Age ?Age; 
              ndar:gender ?Gender; 
              <http://ncitt.ncit.nih.gov/Diagnosis> ?diagnosis . 

  ?bvol_entity prov:wasGeneratedBy ?bvol_act; 
             ?region ?vol .
  FILTER isLiteral(?vol) .
  ?bvol_act prov:wasAssociatedWith ?bvol_soft_agent .
  ?bvol_soft_agent <http://purl.org/nidash/nidm#NIDM_0000164> ?software .

  bind(IF(?Gender="1"^^xsd:string, "Male"^^xsd:string, "Female"^^xsd:string) as ?Gender) .
  FILTER(?diagnosis="Typically Developing Children"^^xsd:string || ?diagnosis="1"^^xsd:string) .

# brain volume stuff#######################################

  ?bvol_entity prov:wasGeneratedBy ?bvol_act; 
             ?region ?vol .
  FILTER isLiteral(?vol)
  ?bvol_act prov:wasAssociatedWith ?bvol_soft_agent .
  ?bvol_soft_agent <http://purl.org/nidash/nidm#NIDM_0000164> ?software .
}